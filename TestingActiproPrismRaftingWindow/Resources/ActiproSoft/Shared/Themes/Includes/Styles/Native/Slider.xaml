<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:controlsPrimitives="clr-namespace:ActiproSoftware.Windows.Controls.Primitives;assembly=ActiproSoftware.Shared.Wpf"
		xmlns:themes="clr-namespace:ActiproSoftware.Windows.Themes;assembly=ActiproSoftware.Shared.Wpf">

	<!-- Geometries -->
	<Geometry x:Key="SliderThumbOuterBorderGeometry">M -5,-9.5 A 1 1 0 0 1 -4,-10.5 L 4,-10.5 A 1 1 0 0 1 5,-9.5 L 5,9.5 A 1 1 0 0 1 4,10.5 L -4,10.5 A 1 1 0 0 1 -5,9.5 Z</Geometry>
	<Geometry x:Key="SliderThumbMiddleBorderGeometry">M -4,-9.5 L 4,-9.5 L 4,9.5 L -4,9.5 Z</Geometry>
	<Geometry x:Key="SliderThumbDisabledGeometry">M -5,-10.5 L 5,-10.5 L 5,10.5 L -5,10.5 Z</Geometry>
	<Geometry x:Key="SliderPointedThumbOuterBorderGeometry">M 4.5,-7.5 A 1 1 0 0 0 3.5,-8.5 L -3.5,-8.5 A 1 1 0 0 0 -4.5,-7.5 L -4.5,4.5 L -0.5,8.5 L 0.5,8.5 L 4.5,4.5 Z</Geometry>
	<Geometry x:Key="SliderPointedThumbMiddleBorderGeometry">M 3.5,-7.5 L -3.5,-7.5 L -3.5,4.5 L 0,8 L 3.5,4.5 Z</Geometry>
	<Geometry x:Key="SliderPointedThumbInnerBorderGeometry">M 2.5,-6.5 L -2.5,-6.5 L -2.5,4.5 L 0,7 L 2.5 4.5 Z</Geometry>
	<Geometry x:Key="SliderPointedThumbDisabledGeometry">M 4.5,-8.5 L -4.5,-8.5 L -4.5,4.5 L -0.5,8.5 L 0.5,8.5 L 4.5,4.5 Z</Geometry>

	<Geometry x:Key="SliderThumbTopAccentGeometry">M 5,-8 L 5,-10 L -4.5,-10 L -4.5,-8 Z</Geometry>
	<Geometry x:Key="SliderThumbBottomAccentGeometry">M 5,8 L 5,10 L -4.5,10 L -4.5,8 Z</Geometry>
	<Geometry x:Key="SliderPointedThumbTopAccentGeometry">M 4.5,-6 L 4.5,-8 L -4.5,-8 L -4.5,-6 Z</Geometry>
	<Geometry x:Key="SliderPointedThumbBottomAccentGeometry">M 4.5,3 L 0,7.5 L -4.5,3 L -4.5,4 L 0,9 L 4.5,4 Z</Geometry>

	<!-- SliderRepeatButtonStyle -->
	<Style x:Key="SliderRepeatButtonStyle" TargetType="{x:Type RepeatButton}">
		<Setter Property="OverridesDefaultStyle" Value="true" />
		<Setter Property="IsTabStop" Value="false" />
		<Setter Property="Focusable" Value="false" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type RepeatButton}">
					<Rectangle Fill="Transparent" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- HorizontalSliderThumbStyle -->
	<Style x:Key="HorizontalSliderThumbStyle" TargetType="{x:Type Thumb}">
		<Setter Property="OverridesDefaultStyle" Value="true" />
		
		<Setter Property="Height" Value="22" />
		<Setter Property="Width" Value="11" />
		
		<Setter Property="Focusable" Value="false" />
		
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type Thumb}">
					<Canvas SnapsToDevicePixels="true">
						<Canvas.RenderTransform>
							<TranslateTransform X="5.5" Y="11" />
						</Canvas.RenderTransform>
						<Path x:Name="background" Data="{StaticResource SliderThumbOuterBorderGeometry}"
								Fill="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBackgroundNormalBrushKey}}" />
						<Path x:Name="innerBorder" Data="{StaticResource SliderThumbMiddleBorderGeometry}"
								Stroke="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalInnerBorderNormalBrushKey}}" />
						<Path x:Name="topHighlight" Data="{StaticResource SliderThumbTopAccentGeometry}"
								Fill="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightNormalBrushKey}}" />
						<Path x:Name="bottomHighlight" Data="{StaticResource SliderThumbBottomAccentGeometry}"
								Fill="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightNormalBrushKey}}" />
						<Path x:Name="outerBorder" Data="{StaticResource SliderThumbOuterBorderGeometry}"
								Stroke="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBorderNormalBrushKey}}" />
					</Canvas>
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter TargetName="background" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBackgroundHoverBrushKey}}" />
							<Setter TargetName="innerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalInnerBorderHoverBrushKey}}" />
							<Setter TargetName="topHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightHoverBrushKey}}" />
							<Setter TargetName="bottomHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightHoverBrushKey}}" />
							<Setter TargetName="outerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBorderHoverBrushKey}}" />
						</Trigger>
						<Trigger Property="Tag" Value="Focused">
							<Setter TargetName="background" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBackgroundFocusedBrushKey}}" />
							<Setter TargetName="innerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalInnerBorderFocusedBrushKey}}" />
							<Setter TargetName="topHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightFocusedBrushKey}}" />
							<Setter TargetName="bottomHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightFocusedBrushKey}}" />
							<Setter TargetName="outerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBorderFocusedBrushKey}}" />
						</Trigger>
						<Trigger Property="IsDragging" Value="true">
							<Setter TargetName="background" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBackgroundPressedBrushKey}}" />
							<Setter TargetName="innerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalInnerBorderPressedBrushKey}}" />
							<Setter TargetName="topHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightPressedBrushKey}}" />
							<Setter TargetName="bottomHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightPressedBrushKey}}" />
							<Setter TargetName="outerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBorderPressedBrushKey}}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="false">
							<Setter TargetName="background" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBackgroundDisabledBrushKey}}" />
							<Setter TargetName="innerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalInnerBorderDisabledBrushKey}}" />
							<Setter TargetName="topHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightDisabledBrushKey}}" />
							<Setter TargetName="bottomHighlight" Property="Fill"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalHighlightDisabledBrushKey}}" />
							<Setter TargetName="outerBorder" Property="Data" Value="{StaticResource SliderThumbDisabledGeometry}" />
							<Setter TargetName="outerBorder" Property="Stroke"
									Value="{DynamicResource {x:Static themes:AssetResourceKeys.SliderThumbHorizontalBorderDisabledBrushKey}}" />
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

