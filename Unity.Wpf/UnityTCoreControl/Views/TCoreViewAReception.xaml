<UserControl x:Class="UnityTCoreControl.Views.TCoreViewAReception"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"  >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBlock Text="{Binding Title}" Foreground="Green" FontFamily="Calibri" FontSize="24" FontWeight="Bold" HorizontalAlignment="Center"  VerticalAlignment="Center" ></TextBlock>
        <Label Content="{Binding RecvMsg}" FontSize="16"  HorizontalAlignment="Center"  VerticalAlignment="Center" Grid.Row="1"/>
        <Label Content="{Binding RecvMsg1}" FontSize="16"  HorizontalAlignment="Center"  VerticalAlignment="Center" Grid.Row="2"/>
    </Grid>
</UserControl>
