<UserControl x:Class="UnityDialogModule.Views.ViewResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/" >
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding Title}" FontSize="24" HorizontalAlignment="Center" />
            <TextBlock Text="{Binding Content}" Margin="10"/>
            <Button Content="OK" Click="Button_Click"  Margin="25"/>
        </StackPanel>
    </Grid>

</UserControl>
